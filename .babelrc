{
  "presets": [
    ["env",{
      "target": {
        "browsers": ["last 2 versions", "safari >= 7"]
      },
      "modules": false,
      "useBuiltIns": true
    }],
    "stage-2",
    "react"
  ],
  "plugins": [
    "transform-decorators-legacy"
  ],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    }
  }
}
